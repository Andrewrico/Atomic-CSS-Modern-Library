{% set pageType = 'Page' %}
<!doctype html>
<html lang="en" class="no-js">
    {% include "partials/head/head.njk" %}
    {% block head %}{% endblock head %}
    <body style="min-height:75vh;color:{{brandColor}}">
        {% include "page/landing/landing-v4/header.njk" %}
        {% block header %}{% endblock header %}
        <main id="main-content" tabindex="-1" 
        style="background:var(--color-light)">
            {% include "page/landing/landing-v4/content.njk" %}
            {% block content %}{% endblock content %}
        </main>
        {% include "partials/footer/footer-v1.njk" %}
        {% block footer %}{% endblock footer%}
        {% include "partials/social-proof/social-proof.njk" %}
    </body>
</html>