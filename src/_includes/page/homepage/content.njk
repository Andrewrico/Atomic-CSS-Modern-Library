{% extends 'layouts/theme.njk' %}
{% block content %}
    <section>
      <div class="D(flex) Fwp(wrap) Mxw(1024x)">
        <div class="F(colspan)">
          <article>
            <div id="preface" class="Fs(19x)">
              {{ content | safe }} 
            </div>
            <div id="{{ sectionOne }}">
              {# {% markdown '/src/_includes/page/homepage/introduction.md' %} #}
            </div>
            <div id="{{ sectionTwo }}">
              {# {% markdown '/src/_includes/page/homepage/md.md' %} #}
            </div>
            <div id="{{ sectionThree }}">
              {# {% markdown '/src/_includes/page/homepage/md.md' %} #}
            </div> 
          <article>
        </div>
        <div class="F(column)">
          <nav class="P(1m)" style="position:sticky;top:0;" aria-label="Secondary Menu"> 
            <h3 class="Fs(1m)"><em>Table of Content</em></h3>
            <ul>
            {% for homepage in indexing.homepage %}
                <li>
                    <a class="Fs(14x)"
                    href="{{ homepage.url }}"}>{{ homepage.text }}</a>
                </li>
            {% endfor %}
            </ul>
          </nav>
        </div>
      </div>
    </section>
{% include "./assets.njk" %} 
{% endblock %}
