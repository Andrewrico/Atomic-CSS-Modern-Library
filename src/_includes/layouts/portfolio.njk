{% set pageType = 'Page' %}

<!doctype html>

<html lang="en" class="no-js">

    {% include "partials/head/head.njk" %}
    {% block head %}{% endblock head %}

    <body style="min-height:70vh;">

        {% include "partials/navigation/navigation.njk" %}
        {% block nav %}{% endblock nav %}

        {% include "page/portfolio/header.njk" %}
        {% block header %}{% endblock header %}

        <main id="main-content" tabindex="-1">

            {% if page.url == '/affiliated-networks/' %}
                {% include "page/portfolio/affiliated-networks.njk" %} 
            {% endif %}  

            {% if page.url == '/make-money-online/' %}
                {% include "page/portfolio/make-money-online.njk" %} 
            {% endif %}  
  
            {% block content %}{% endblock content %}

        </main>

        {% include "partials/footer/footer.njk" %}
        {% block footer %}{% endblock footer%}
        
    </body>

</html>
